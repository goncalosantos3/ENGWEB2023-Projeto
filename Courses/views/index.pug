extends layout

block content
  .w3-card-4.w3-margin

    header.w3-container.w3-blue
      h1 Education Platform

    .w3-container
      h2 News 
      ul 
        each course in courses 
          li #[a(href="/" + course.id) #{course.name}]
            ul
              each n in course.news 
                li #[b #{n.description}]
              
    footer.w3-container.w3-blue
      h5 Generated by EngWeb2023 in #{d}